{
  "name": "backend",
  "version": "1.0.0",
  "private": true,
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "start": "npm run build && node dist/main.js",
    "start:dev": "cross-env DOTENV_CONFIG_PATH=env.example node --watch --enable-source-maps -r ts-node/register -r reflect-metadata -r dotenv/config src/main.ts",
    "db:migrate": "cross-env DOTENV_CONFIG_PATH=env.example ts-node -r dotenv/config scripts/migrate.ts",
    "db:seed": "cross-env DOTENV_CONFIG_PATH=env.example ts-node -r dotenv/config scripts/seed.ts",
    "test": "jest --config jest.config.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/websockets": "^11.1.11",
    "@prisma/client": "^6.19.1",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "dotenv": "^17.2.3",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.3",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.14",
    "@prisma/engines": "^7.2.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "cross-env": "^10.1.0",
    "jest": "^30.2.0",
    "prisma": "^6.19.1",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
